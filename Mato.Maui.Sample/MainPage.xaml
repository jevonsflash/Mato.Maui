<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:MatoRichTextEditor.Views;assembly=Mato.Maui.Sample"
             x:Class="MatoRichTextEditor.MainPage">

    <TableView Intent="Menu">
        <TableRoot>
            <TableSection Title="Controls">
                <TextCell Text="RichTextEditor Sample"
                          Command="{Binding NavigateCommand}"
                          CommandParameter="{x:Type views:RichTextEditorSamplePage}" />
            </TableSection>
        </TableRoot>
    </TableView>

</ContentPage>

